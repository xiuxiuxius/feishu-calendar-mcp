  "repository": {
    "type": "git",
    "url": "https://github.com/xiuxiuxius/feishu-calendar-mcp.git"
  },
  "homepage": "https://github.com/xiuxiuxius/feishu-calendar-mcp#readme",
  "bugs": {
    "url": "https://github.com/xiuxiuxius/feishu-calendar-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "dotenv": "^17.2.3"
