{
  "name": "feishu-calendar-mcp",
  "version": "1.3.0",
  "description": "MCP server for Feishu Calendar API - Integrates Feishu/Lark calendar with Claude Desktop",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "feishu-calendar-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "TOKEN_GUIDE.md",
    ".env.example"
